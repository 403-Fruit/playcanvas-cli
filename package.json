{
  "name": "playcanvas-tools",
  "version": "0.4.6",
  "license": "MIT",
  "scripts": {
    "start": "yarn copy-template && tsc --project . && node ./built/bin/playcanvas-cli.js",
    "lint": "eslint --fix scripts",
    "build": "yarn copy-template && tsc --project .",
    "copy-template": "rm -rf built && mkdir built && cp -R src/template/ built/template",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "built",
    "scripts",
    "bin",
    "template"
  ],
  "keywords": [
    "REST API",
    "playcanvas",
    "deploy"
  ],
  "bin": {
    "playcanvas-cli": "built/bin/playcanvas-cli.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@types/fs-extra": "^8.0.0",
    "axios": "^0.19.0",
    "core-js": "3.2.1",
    "cross-spawn": "^7.0.0",
    "dotenv": "^8.1.0",
    "extract-zip": "^1.6.7",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
    "isomorphic-crypto": "^3.0.0",
    "jsdom": "^15.1.1",
    "meow": "^5.0.0",
    "playcanvas-node": "1.3.0",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@types/axios": "^0.14.0",
    "@types/extract-zip": "^1.6.2",
    "@types/inquirer": "^6.5.0",
    "@types/jsdom": "^12.2.4",
    "@types/meow": "^5.0.0",
    "@types/node": "^12.7.5",
    "@types/progress": "^2.0.3",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.42",
    "typescript": "^3.6.3"
  }
}
